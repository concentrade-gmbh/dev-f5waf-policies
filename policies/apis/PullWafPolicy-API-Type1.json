{
    "policy": {
        "name": "svs-DeclarativePullWafPolicy-API-Type1",
        "description": "Policy is an API policy, receiving an external OpenAPI schema",
        "template": {
            "name": "POLICY_TEMPLATE_API_SECURITY"
        },
        "applicationLanguage": "utf-8",
        "enforcementMode": "blocking",
        "server-technologies": [
            {
                "serverTechnologyName": "JavaScript"
            },
            {
                "serverTechnologyName": "Unix/Linux"
            },
            {
                "serverTechnologyName": "jQuery"
            }
        ],
        "signature-settings": {
            "signatureStaging": false
        },
        "policy-builder": {
            "learnOnlyFromNonBotTraffic": false
        },
        "open-api-files": [
            {
                "link": "https://raw.githubusercontent.com/concentrade-gmbh/dev-f5waf-policies/main/schema/Arcadia-OAS3-2.0.0-oas3.json"
            }
        ]
    }
}